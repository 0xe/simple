// only integer exponentials
let expt = function(a, n) {
    if (n == 0) {
        return 1;
    } else {
        let e = 1; let i = 0;
        while (i < n) {
            i = i+1;
            e = e*a;
        }
        return e;
    }
};

let two_raised_to_sixteen_msg = "Two raised to sixteen squared is: ";
let two_raised_to_sixteen = expt(2, 256);
print(two_raised_to_sixteen_msg, two_raised_to_sixteen);
